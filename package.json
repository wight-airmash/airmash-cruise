{
  "name": "airmash-cruise",
  "version": "1.0.2",
  "description": "",
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "dev": "rollup -w -c"
  },
  "keywords": [],
  "author": "wight",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:import/errors",
      "plugin:import/warnings",
      "prettier"
    ],
    "plugins": [
      "import"
    ],
    "settings": {
      "import/extensions": [
        ".js",
        ".json"
      ]
    },
    "rules": {
      "no-console": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never"
        }
      ]
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "SWAM": false,
      "SettingsProvider": false,
      "game": false,
      "config": false,
      "Players": false,
      "Network": false,
      "chrome": false,
      "$": false,
      "Base64": false,
      "Input": false
    }
  },
  "eslintIgnore": [
    "dist/*"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "prettier": "^1.16.4",
    "rollup": "^1.1.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-terser": "^4.0.4"
  },
  "dependencies": {}
}
